        @{
    var profiler2 = Context.GetProfiler();  
    if(profiler2 != null)
    {   // developer; show what timings we have
        profiler2.EndAllImpl();
                
@profiler2.Render();

   }
        
}


            @RenderBody()
        </div>
        <div id="footer">Site Design / Logo C StackExpertsExchange   inc, blah blah blah<br />
@{
    var profiler = Context.GetProfiler();  
    if(profiler != null)
    {   // developer; show what timings we have
