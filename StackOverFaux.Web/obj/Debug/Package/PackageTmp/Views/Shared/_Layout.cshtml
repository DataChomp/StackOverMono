@{
    var profiler = MiniProfiler.Current;
}
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Public/css/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Public/scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.12/jquery-ui.min.js" type="text/javascript"></script>
    <link href="@Url.Content("~/Public/css/jquery-ui-1.8.12.custom.css")" rel="stylesheet" type="text/css" />

@using (profiler.Step("<head> rendering"))
{
    @MiniProfiler.RenderIncludes(position: RenderPosition.Left, showTrivial: false, showTimeWithChildren: false)

}

</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>Stack Over-Faux</h1>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <div id="menucontainer">
                <ul id="menu">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Badges", "Badges", "Home")</li>
                    <li>@Html.ActionLink("User", "DisplayUser", "Home")</li>
                    <li>@Html.ActionLink("Tags", "Tags", "Home")</li>
                </ul>
            </div>
        </div>
        <div id="main">


            @RenderBody()
        </div>
        <div id="footer">Site Design / Logo C StackExpertsExchange   inc, blah blah blah<br />

        </div>
    </div>
</body>
</html>
